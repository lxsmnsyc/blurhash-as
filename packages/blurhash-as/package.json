{
  "name": "blurhash-as",
  "version": "0.10.0",
  "type": "module",
  "types": "index.d.ts",
  "main": "index.cjs",
  "module": "index.mjs",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./browser": {
      "require": "./browser.cjs",
      "import": "./browser.mjs",
      "types": "./browser.d.ts"
    },
    "./build/*": "./build/*"
  },
  "files": [
    "assembly",
    "build",
    "browser.d.ts",
    "browser.mjs",
    "browser.cjs",
    "index.cjs",
    "index.d.ts",
    "index.mjs"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": ["blurhash", "wasm", "webassembly", "assemblyscript"],
  "license": "MIT",
  "dependencies": {
    "@assemblyscript/loader": "^0.27.23"
  },
  "devDependencies": {
    "assemblyscript": "^0.27.23",
    "benny": "^3.6.15",
    "blurhash": "^1.1.3",
    "image-encode": "^1.3.1",
    "image-pixels": "^2.2.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "asbuild:untouched": "asc assembly/index.ts --target debug --runtime minimal --exportRuntime",
    "asbuild:optimized": "asc assembly/index.ts --target release --runtime minimal --exportRuntime",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "test": "node tests/index.cjs"
  },
  "description": "Blurhash algorithm implementation in AssemblyScript",
  "repository": {
    "url": "https://github.com/lxsmnsyc/blurhash-as.git",
    "type": "git"
  },
  "homepage": "https://github.com/lxsmnsyc/blurhash-as/tree/main/packages/blurhash-as",
  "bugs": {
    "url": "https://github.com/lxsmnsyc/blurhash-as/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Alexis Munsayac",
  "private": false
}
